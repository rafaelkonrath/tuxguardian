first: all
####### Implicit rules

.SUFFIXES: .c .o .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	test -d /usr/local/bin/ || mkdir -p /usr/local/bin/
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(OBJCOMP) $(LIBS)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: tg-frontend.pro  /usr/share/qt3/mkspecs/default/qmake.conf /usr/share/qt3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile tg-frontend.pro
qmake: 
	@$(QMAKE) -o Makefile tg-frontend.pro

dist: 
	@mkdir -p .obj/tg-frontend && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/tg-frontend/ && $(COPY_FILE) --parents ask.ui.h .obj/tg-frontend/ && ( cd `dirname .obj/tg-frontend` && $(TAR) tg-frontend.tar tg-frontend && $(GZIP) tg-frontend.tar ) && $(MOVE) `dirname .obj/tg-frontend`/tg-frontend.tar.gz . && $(DEL_FILE) -r .obj/tg-frontend

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) /usr/local/bin/$(TARGET) $(TARGET)


FORCE:

####### Compile

.obj/main.o: main.cpp main.h \
		trayicon.h \
		tg-icon.xpm \
		.ui/ask.h \
		md5.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.obj/md5.o: md5.c md5.h \
		unlocked-io.h
	$(CC) -c $(CFLAGS) $(INCPATH) -o .obj/md5.o md5.c

.obj/trayicon.o: trayicon.cpp trayicon.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/trayicon.o trayicon.cpp

.obj/trayicon_x11.o: trayicon_x11.cpp trayicon.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/trayicon_x11.o trayicon_x11.cpp

.ui/ask.h: ask.ui 
	$(UIC) ask.ui -o .ui/ask.h

.ui/ask.cpp: .ui/ask.h ask.ui ask.ui.h 
	$(UIC) ask.ui -i ask.h -o .ui/ask.cpp

.obj/ask.o: .ui/ask.cpp ask.ui.h \
		.ui/ask.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/ask.o .ui/ask.cpp

.obj/moc_main.o: .moc/moc_main.cpp  main.h .ui/ask.h \
		md5.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_main.o .moc/moc_main.cpp

.obj/moc_trayicon.o: .moc/moc_trayicon.cpp  trayicon.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_trayicon.o .moc/moc_trayicon.cpp

.obj/moc_ask.o: .moc/moc_ask.cpp  .ui/ask.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_ask.o .moc/moc_ask.cpp

.moc/moc_main.cpp: $(MOC) main.h
	$(MOC) main.h -o .moc/moc_main.cpp

.moc/moc_trayicon.cpp: $(MOC) trayicon.h
	$(MOC) trayicon.h -o .moc/moc_trayicon.cpp

.moc/moc_ask.cpp: $(MOC) .ui/ask.h
	$(MOC) .ui/ask.h -o .moc/moc_ask.cpp

