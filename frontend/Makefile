		trayicon.h
SOURCES = main.cpp \
		md5.c \
		trayicon.cpp \
		trayicon_x11.cpp
OBJECTS = .obj/main.o \
		.obj/md5.o \
		.obj/trayicon.o \
		.obj/trayicon_x11.o \
		.obj/ask.o
FORMS = ask.ui
UICDECLS = .ui/ask.h
UICIMPLS = .ui/ask.cpp
SRCMOC   = .moc/moc_main.cpp \
		.moc/moc_trayicon.cpp \
		.moc/moc_ask.cpp
OBJMOC = .obj/moc_main.o \
		.obj/moc_trayicon.o \
		.obj/moc_ask.o
